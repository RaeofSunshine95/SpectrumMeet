@model IEnumerable<SpectrumMeetEF.User>
<h4>Results:</h4>
<table class="table table-striped table-bordered border-light-subtle">
    <thead class="table-primary">
        <tr>
            <th>
                @Html.DisplayName("First Name")
            </th>
            <td>
                @Html.DisplayName("Last Name")
            </td>
            <th>
                @Html.DisplayName("City")
            </th>
            <th>
                @Html.DisplayName("State")
            </th>
            <th>
                @Html.DisplayName("Username")
            </th>
        </tr>
    </thead>
    <tbody class="table-secondary">
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FirstName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LastName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.City)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.State)
                </td>
                <td>
                    @Html.ActionLink(item.Account.Username, "Details", new { controller = "Users", id = item.AccountID })
                </td>
            </tr>
        }
    </tbody>
</table>
